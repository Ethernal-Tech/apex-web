{
  "app": {
    "name": "apex-bridge",
    "logLevel": "debug",
    "port": 30000,
    "isMainnet": false
  },
  "features": {
    "useCentralizedBridge": false
  },
  "bridge": {
    "ethTxTtlInc": 50,
    "recentInputsThresholdMinutes": 5,
    "addresses": {
      "nexusBridging": "0xc68221AD72397d85084f2D5C7089e4e9487c118c",
      "nexusCentralizedBridging": "0xEe639cDA5D46Bd32D0013bB75f60F3E691D9839f"
    }
  },
  "services": {
    "oracleUrl": "http://localhost:40000",
    "cardanoApiUrl": "http://localhost:41000",
    "centralizedApiUrl": "https://developers.apexfusion.org"
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "ssl": false,
    "migrationsTableName": "__apex_migrations",
    "entities": ["dist/**/*.entity.js"],
    "migrations": ["dist/database/migrations/*.js"]
  }
}
