{
  "app": {
    "name": "apex-bridge",
    "logLevel": "debug",
    "port": 30000,
    "corsAllowList": [
      "https://reactor.testnet.apexfusion.org",
      "https://skylinebridge.tech"
    ],
    "isMainnet": false
  },
  "features": {
    "useCentralizedBridge": false
  },
  "bridge": {
    "ethTxTtlInc": 50,
    "recentInputsThresholdMinutes": 5
  },
  "services": {
    "oracleUrl": "http://bridge-api-testnet.apexfusion.org:10003",
    "cardanoApiUrl": "https://cardano-api.ethernal.tech",
    "centralizedApiUrl": "https://developers.apexfusion.org"
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "ssl": false,
    "migrationsTableName": "__apex_migrations",
    "entities": ["dist/**/*.entity.js"],
    "migrations": ["dist/database/migrations/*.js"]
  }
}